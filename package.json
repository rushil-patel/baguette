{
  "name": "baguette",
  "version": "1.0.0",
  "description": "a better get, that works with objects and arrays",
  "main": "src/index.ts",
  "module": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "author": "Rushil Patel",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rimraf lib",
    "test": "yarn lint:test && yarn cover",
    "test:only": "node_modules/ts-mocha/bin/ts-mocha -p ./tsconfig.testing.json tests/**/*.spec.ts",
    "test:watch": "yarn test -- --watch",
    "cover": "nyc --check-coverage yarn test:only",
    "lint": "standard --fix",
    "lint:test": "standard --fix --env mocha",
    "build": "yarn clean && cross-env BABEL_ENV=production rollup -c"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.8",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "standard": "^14.3.4",
    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.2"
  }
}
